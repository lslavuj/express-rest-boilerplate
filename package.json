{
  "name": "express-rest-boilerplate",
  "version": "1.0.0",
  "description": "Express typescript rest boilerplate",
  "main": "index.ts",
  "scripts": {
    "initDatabases": "npm run dockerCompose && docker stop testDbContainer && npm run migration:run && npm run seed",
    "build": "rimraf ./dist && tsc",
    "check-branch-name": ".husky/check-branch-name.sh",
    "prepare": "husky install",
    "start": "npm run build && node dist/app.js",
    "dockerCompose": "docker compose up --wait",
    "start:dev": "nodemon ./bin/www/server",
    "test": "jest -runInBand --collectCoverage",
    "test:watch": "jest --watch -runInBand --collectCoverage --detectOpenHandles",
    "typeorm": "typeorm-ts-node-esm",
    "migration:run": "npm run typeorm migration:run -- -d ./src/database/config",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/database/config",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/database/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lslavuj/express-rest-boilerplate.git"
  },
  "author": {
    "name": "Luka Slavuj",
    "email": "lukaslavuj@gmail.com",
    "url": "https://www.linkedin.com/in/luka-slavuj"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lslavuj/express-rest-boilerplate/issues"
  },
  "homepage": "https://github.com/lslavuj/express-rest-boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-terminator": "^3.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.2.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "typeorm": "^0.3.12",
    "typeorm-extension": "^2.6.2",
    "ua-parser-js": "^1.0.34",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.10",
    "@types/request-ip": "^0.0.37",
    "@types/ua-parser-js": "^0.7.36",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.0.4",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
}
