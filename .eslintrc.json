{
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": ["airbnb-base", "plugin:import/recommended", "plugin:import/typescript", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "import"],
  "rules": {},
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  }
}
